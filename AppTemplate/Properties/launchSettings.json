{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "https://localhost:44321/",
      "sslPort": 44321
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_preventHostingStartup": "True",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "AppTemplate": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Setup Db": {
      "commandName": "Project",
      "commandLineArgs": "tools migrate seed",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Update Client": {
      "commandName": "Project",
      "commandLineArgs": "tools \"clientgen Client\\Libs\\ServiceClient.ts\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Update Config Schema": {
      "commandName": "Project",
      "commandLineArgs": "tools updateConfigSchema",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "createCert": {
      "commandName": "Project",
      "commandLineArgs": "tools \"createCert AppCert.pfx AppCert.pem\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}